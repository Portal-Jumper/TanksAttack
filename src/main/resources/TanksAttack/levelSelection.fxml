<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TanksAttack.LevelSelectionController">
   <children>
      <ImageView fitHeight="420.0" fitWidth="620.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@levelSelectionBackground.jpg" />
         </image>
      </ImageView>
      <Button fx:id="button1" layoutX="80.0" layoutY="300.0" mnemonicParsing="false" onMouseClicked="#level1" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-inset: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Level 1" />
      <Pane fx:id="pane2" layoutX="200.0" layoutY="300.0" prefHeight="35.0" prefWidth="80.0">
         <children>
            <Button fx:id="button2" mnemonicParsing="false" onMouseClicked="#level2" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-inset: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Level 2" />
         </children>
      </Pane>
      <Pane fx:id="pane3" layoutX="318.0" layoutY="300.0" prefHeight="35.0" prefWidth="80.0">
         <children>
            <Button fx:id="button3" mnemonicParsing="false" onMouseClicked="#level3" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-inset: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Level 3" />
         </children>
      </Pane>
      <Pane fx:id="pane4" layoutX="440.0" layoutY="300.0" prefHeight="35.0" prefWidth="80.0">
         <children>
            <Button fx:id="button4" mnemonicParsing="false" onMouseClicked="#level4" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-inset: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Level 4" />
         </children>
      </Pane>
      <Button fx:id="buttonRandom" layoutX="150.0" layoutY="350.0" mnemonicParsing="false" onMouseClicked="#randomLevel" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-inset: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Random Level" />
      <Label fx:id="pointsLabel" alignment="CENTER" layoutX="150.0" layoutY="140.0" text="Points gathered so far: 1000" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="25.0" />
         </font>
      </Label>
      <Label fx:id="difficultyLane1" layoutX="300.0" layoutY="20.0" text="Easy Difficulty:" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="19.0" />
         </font>
      </Label>
      <Label fx:id="difficultyLane2" layoutX="300.0" layoutY="50.0" text="Your movement speed: 1.9" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="19.0" />
         </font>
      </Label>
      <Label fx:id="difficultyLane3" layoutX="300.0" layoutY="80.0" text="Enemy movement speed: 1" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="19.0" />
         </font>
      </Label>
      <Label fx:id="difficultyLane4" layoutX="300.0" layoutY="110.0" text="Enemies shoot every: 1,5sec" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="19.0" />
         </font>
      </Label>
      <ChoiceBox id="choice-box" fx:id="levelChoiceBox" layoutX="141.0" layoutY="20.0" prefWidth="150.0" stylesheets="@choice-box.css" />
      <Label fx:id="difficultyLane21" layoutX="15.0" layoutY="70.0" text="Moving: WASD" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="19.0" />
         </font>
      </Label>
      <Label fx:id="difficultyLane211" layoutX="15.0" layoutY="100.0" text="Shooting: L" textFill="WHITE">
         <font>
            <Font name="Lato Regular" size="19.0" />
         </font>
      </Label>
      <Button fx:id="saveButton" layoutX="30.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#saveToDB" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-inset: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Save" />
      <Button fx:id="ranking" layoutX="350.0" layoutY="350.0" mnemonicParsing="false" onAction="#ranking" style="-fx-background-color: #090a0c; -fx-background-color: linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%); -fx-background-color: linear-gradient(#20262b, #191d22); -fx-background-radius: 5,4,3,5; -fx-background-insets: 0,1,2,0; -fx-text-fill: white; -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 ); -fx-font-family: Arial; -fx-text-fill: linear-gradient(white, #d0d0d0); -fx-font-size: 12px; -fx-padding: 10 20 10 20;" text="Ranking" textFill="TRANSPARENT" />
   </children>
</AnchorPane>
